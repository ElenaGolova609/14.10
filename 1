#include <iostream>
#include <vector>
#include <algorithm>

std::vector<int> removeM(const std::vector<int>&  input) {
    if (input.empty()) return {};
    
    std::vector<int> result = input;
    
    std::sort(result.begin() , result.end());
    auto last = std::unique(result.begin() , result.end());
    result.erase(last, result.end());
    
    return result;
}

int main(){
    std::vector<int> massiv;
    
    int r;
    std::cout << "Введите размер массива" << std::endl;
    std::cin >> r;
    
    std::cout << "Введите числа(желательно много одинаковых)" << std::endl;
    for (int i = 0; i < r; ++i) {
        int chisla;
        std::cin >> chisla;
        massiv.push_back(chisla);
    }
    
    std::vector<int> newMassiv = removeM(massiv);
    
    std::cout << "Нынешний массив" << std::endl;
    for (int chisla : massiv) {
        std::cout << chisla << ' ';
    }
    
    std::cout << "\n";
    
    std::cout << "Без дублей " << std::endl;
    for(int chisla : newMassiv) {
        std::cout << chisla << " ";
    }
    
    std::cout << "\n";
    
}
