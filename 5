#include <iostream>
#include <cstdint>  

void chislo(uint64_t a) {
    for (uint64_t i = 1; i <= a; i++) {
        std::cout << i << " ";
    }
    std::cout << std::endl;
}

int main() {
    uint64_t c;
    
    std::cout << "Введите число a(больше 1): " << std::endl;
    std::cin >> c;
    
    std::cout << "Все числа до " << c << ": ";
    chislo(c);
    
    return 0;
}
